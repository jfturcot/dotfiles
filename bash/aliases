# Shell
alias ll="ls -l"
alias la="ls -la"

# Processes
alias tu="top -o cpu" 						# cpu
alias tm="top -o vsize"						# memory

# Ruby
alias be="bundle exec"

# Git
alias g="git status"
alias ga="git add"
alias gaa="git add -A"
alias gc="git commit"
alias gca="git commit -a"
alias gcm="git commit -m"
alias gd="git diff"
alias gp="git push"
alias gb="git branch"

# Heroku
alias heroc="ruby /Users/jfturcot/.heroku/credentials.rb"
alias heros="ruby /Users/jfturcot/.heroku/switcher.rb"

# tmux
alias tmux="TERM=screen-256color-bce tmux"

